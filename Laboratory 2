#BSIS2-DV-LAB2-JAYSONB
print("Enter the first number:")
fnum = input()
print("Enter the second number:")
snum = input()
print()

fnum = float(fnum)
snum = float(snum)

sum = fnum+snum
diff = fnum-snum
prod = fnum*snum
quot = fnum/snum
pow = fnum**snum
dwr = fnum//snum
r = fnum%snum

print(f"Sum: {sum}")
print(f"Difference: {diff}")
print(f"Product: {prod}" )
print(f"Quotient: {quot}")
print(f"Power: {pow}")
print(f"Division with Remainder: {dwr}")
print(f"Remainder: {r}")


#BSIS2-DV-LAB2-JAYSONB
